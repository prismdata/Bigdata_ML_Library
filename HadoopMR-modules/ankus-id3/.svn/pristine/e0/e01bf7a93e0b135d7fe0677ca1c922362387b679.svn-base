{
  "packagename":"org.ankus.mapreduce.algorithms.classification.id3",
  "author":"onycom",
  "create":"2016-04-14",
  "applist":[
	{
	"appname":"ID3 Classification", 
	"appicon":"res/images/ml_classify_id3.png",		
	"classname":"ID3",
	"version": "0.1",
	"appgroup": "CLASSIFICATION",	
	"nexus":"ankus-core2-id3-1.1.0.jar",	
	"params":[	
			{
			 "type":"boolean",
			 "name":"suppliedTest",
			 "description":"Supplied Test Set",
			 "required":"Y",
			 "values":[
			       {"value":"true","option":"True"},
				   {"value":"false","option":"False"}
				],
			 "iotype":"D"			 
			}, 
			{
			 "type":"subinput",
			 "name":"modelPath",
			 "description":"Input model path",
			 "required":"N",
			 "filter":"suppliedTest=='true'",
			 "iotype":"I"			 
			}, 
			{
			 "type":"columnindex",
			 "name":"indexList",
			 "description":"Target Attributes(Index) List",
			 "required":"Y",
			 "iotype":"I"		 
			},
			{
			 "type":"columnindex",
			 "name":"exceptionIndexList",
			 "description":"Exception Attributes(Index) List",
			 "required":"N",	
			 "iotype":"I"		 
			},
			{
			 "type":"columnselect",
			 "name":"classIndex",
			 "description":"Class Attribute(Index)",
			 "required":"Y",			
			 "iotype":"I"		 
			},
			{
			 "type":"text",
			 "name":"minLeafData",
			 "description":"Pruning - Minimum Data Count",	
			 "required":"Y",
			 "values":"2",
			 "filter":"suppliedTest=='false'",	
			 "iotype":"I"		 
			},
			{
			 "type":"text",
			 "name":"purity",
			 "description":"Pruning - Leaf Node Purity",	
			 "required":"Y",
			 "values":"0.75",
			 "filter":"suppliedTest=='false'",	
			 "iotype":"I"		 
			},
			{
			 "type":"boolean",
			 "name":"finalResultGen",
			 "description":"Training Result Generation",
			 "required":"Y",
			 "values":[
			       {"value":"true","option":"True"},
				   {"value":"false","option":"False"}
				],
			 "filter":"suppliedTest=='false'",	
			 "iotype":"I"		
			}	
		]	
	}
  ]
}