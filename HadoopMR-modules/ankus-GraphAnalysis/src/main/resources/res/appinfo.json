	{
	"appname":"ETL", 
	"appicon":"res/images/ETL.png",	
	"classname":"ETL",
	"version": "0.1",
	"appgroup": "PREPROCESSING",	
	"nexus":"org.ankus:ankus-core2-onycom-alpha1:1.1.0",	
	"params":[		
			{
			 "type":"combo",
			 "name":"etlMethod",
			 "description":"Method",
			 "required":"N",
			 "values":[
			       {"value":"ColumnExtractor","option":"Column Extractor"},
				   {"value":"FilterInclude","option":"Filter Include"},
				   {"value":"FilterExclude","option":"Filter Exclude"},
				   {"value":"Replace","option":"Replace"},
				   {"value":"NumericNorm","option":"Categorization"},
				   {"value":"RowSort","option":"Rows Sort"}
				   ], 
			 "iotype":"I"		 
			},
			{
			 "type":"columnindex",
			 "name":"indexList",
			 "description":"Include Attributes(Index) List",
			 "required":"Y",
			 "filter":"etlMethod=='ColumnExtractor'",
			 "iotype":"I"		
			},
			{
			 "type":"boolean",
			 "name":"ruleType",
			 "description":"Rule Type",
			 "required":"Y",
			 "values":[
			       {"value":"file","option":"File"},
				   {"value":"text","option":"Text"}
				],
			 "filter":"etlMethod=='FilterInclude' || etlMethod=='FilterExclude' || etlMethod=='Replace' || etlMethod=='NumericNorm'",		
			 "iotype":"D"		
			},
			{
			 "type":"subinput",
			 "name":"filterRulePath",
			 "description":"Rule(File)",
			 "required":"N",
			 "filter":"(etlMethod=='FilterInclude' || etlMethod=='FilterExclude' ) && ruleType == 'file'",	
			 "iotype":"I"			 
			},
			{
			 "type":"text",
			 "name":"filterRule",
			 "description":"Rule(Text) <br>ex: 'Column ID,value1|Column ID,value2' ",
			 "required":"N",	
			 "filter":"(etlMethod=='FilterInclude' || etlMethod=='FilterExclude' ) && ruleType == 'text'",
			 "iotype":"I"		 
			},
			{
			 "type":"subinput",
			 "name":"ReplaceRulePath",
			 "description":"Rule(File)",
			 "required":"N",
			 "filter":"(etlMethod=='Replace') && ruleType == 'file'",	
			 "iotype":"I"			 
			},
			{
			 "type":"text",
			 "name":"ReplaceRule",
			 "description":"Rule(Text) <br>ex:'Column ID, RegExpress, New Value' ",
			 "required":"N",	
			 "filter":"(etlMethod=='Replace') && ruleType == 'text'",
			 "iotype":"I"		 
			},
			{
			 "type":"subinput",
			 "name":"NumericFormFile",
			 "description":"Rule(File)",
			 "required":"Y",
			 "filter":"(etlMethod=='NumericNorm') && ruleType == 'file'",	
			 "iotype":"I"			 
			},
			{
			 "type":"text",
			 "name":"NumericForm",
			 "description":"Rule(Text) <br> ex: 'var1<=x<=var2->New Value'",
			 "required":"Y",	
			 "filter":"(etlMethod=='NumericNorm') && ruleType == 'text'",
			 "iotype":"I"
			},
			{
			 "type":"combo",
			 "name":"Sort",
			 "description":"Sort row order",
			 "required":"N",
			 "values":[
			       {"value":"asc","option":"Ascending"},
				   {"value":"desc","option":"Descending"}
				   ], 
			 "filter":"etlMethod =='RowSort'",
			 "iotype":"I"
			},
			{
			 "type":"text",
			 "name":"SortTarget",
			 "description":"Sort index",
			 "required":"N",
			 "filter":"etlMethod =='RowSort'",
			 "iotype":"I"		 
			}					
		]
	},	
