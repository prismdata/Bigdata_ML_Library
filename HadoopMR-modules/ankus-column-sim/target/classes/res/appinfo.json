{
  "packagename":"org.ankus.mapreduce",
  "author":"onycom",
  "create":"2016-04-14",
  "applist":[
	{
	"appname":"Column based Similarity/Correlationn", 
	"appicon":"res/images/simcorr_column.png",		
	"classname":"ColumnCorrelation",
	"version": "0.1",
	"appgroup": "CORRELATION",
	"nexus":"ankus-core2-column-sim-1.1.0.jar",	
	"params":[		
			{
			 "type":"columnindex",
			 "name":"indexList",
			 "description":"Target Attributes(Index) List",
			 "required":"Y",
			 "iotype":"I"		 
			},
			{
			 "type":"columnindex",
			 "name":"exceptionIndexList",
			 "description":"Exception Attributes(Index) List",
			 "required":"N",			
			 "iotype":"I"		 
			},
			{
			 "type":"combo",
			 "name":"algorithmOption",
			 "description":"Similarity/Correlation Algorithm",
			 "required":"N",
			 "values":[
				   {"value":"hamming","option":"HAMMING DISTANCE"},				
				   {"value":"dice","option":"DICE COEFFICIENT"},
				   {"value":"jaccard","option":"JACCARD COEFFICIENT"},
				   {"value":"tanimoto","option":"TANIMOTO COEFFICIENT"},
				   {"value":"manhatan","option":"MANHATAN DISTANCE"},
				   {"value":"uclidean","option":"UCLIDEAN DISTANCE"},
				   {"value":"cosine","option":"COSINE COEFFICIENT"},
				   {"value":"pearson","option":"PEARSON COEFFICIENT"},
				   {"value":"matching","option":"MATCHING COEFFICIENT"},
				   {"value":"edit","option":"EDIT DISTANCE"}
				   ], 
			 "iotype":"I"		 
			}		
		]	
	}
  ]
}