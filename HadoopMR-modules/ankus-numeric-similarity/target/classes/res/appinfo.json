  "applist":[
	{
	"appname":"Numeric Similarity/Correlation", 
	"appicon":"res/images/simcorr_row_numeric.png",		
	"classname":"NumericStatistics",
	"version": "0.1",
	"appgroup": "CORRELATION",	
	"nexus":"ankus-core2-numeric-similarity-1.1.0.jar",	
	"params":[		
			{
			 "type":"columnindex",
			 "name":"keyIndex",
