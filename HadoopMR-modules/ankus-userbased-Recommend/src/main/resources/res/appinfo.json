{
  "packagename":"onycom_Analyzer_package",
  "author":"onycom",
  "create":"2016-04-14",
  "applist":[
	{
	"appname":"User/Item Similarity based Recommendation", 
	"appicon":"res/images/recom_gen.png",		
	"classname":"Recommendation",
	"version": "0.1",
	"appgroup": "RECOMMENDATION",	
	"nexus":"ankus-core2-userbasedRecommend-1.1.0.jar",	
	"params":[		
			{
			 "type":"columnindex",
			 "name":"uidIndex",
			 "description":"User ID Index",
			 "required":"Y",
			 "iotype":"I"		 
			},
			{
			 "type":"columnindex",
			 "name":"iidIndex",
			 "description":"Item ID Index",
			 "required":"Y",			
			 "iotype":"I"		 
			},
			{
			 "type":"columnindex",
			 "name":"ratingIndex",
			 "description":"Rating Value Index",
			 "required":"Y",			
			 "iotype":"I"		 
			},
			{
			 "type":"subinput",
			 "name":"similPath",
			 "description":"Similarity Path",
			 "required":"N",			 
			 "iotype":"I"			 
			},
			{
			 "type":"delimeter",
			 "name":"similDelimiter",
			 "description":"Similarity Column Delimeter",
			 "required":"N",			
			 "iotype":"I"		 
			},
			{
			 "type":"combo",
			 "name":"basedType",
			 "description":"Similarity Base Type",
			 "required":"N",
			 "values":[
				   {"value":"user","option":"USER"},				
				   {"value":"item","option":"ITEM"}				
				   ], 
			 "iotype":"I"		 
			},
			{
			 "type":"text",
			 "name":"similThreshold",
			 "description":"Similarty Threshold",
			 "required":"N",
			 "values":"0.7",
			 "iotype":"I"		
			},
			{
			 "type":"text",
			 "name":"targetUID",
			 "description":"Recommend Target User ID",
			 "required":"N",
			 "iotype":"I"		
			},
			{
			 "type":"text",
			 "name":"targetIIDList",
			 "description":"Recommend Target Item ID List",
			 "required":"N",
			 "iotype":"I"		
			},
			{
			 "type":"text",
			 "name":"recomCnt",
			 "description":"Top-N Recommendation",
			 "required":"N",
			 "values":"10",
			 "iotype":"I"		
			}			
		]	
	}
  ]
}