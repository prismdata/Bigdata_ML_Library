{
  "packagename":"org.ankus.mapreduce.algorithms.correlation.stringset",
  "author":"onycom",
  "create":"2016-04-14",
  "applist":[
	{
	"appname":"String Similarity", 
	"appicon":"res/images/simcorr_row_string.png",		
	"classname":"StringDataCorrelation",
	"version": "0.1",
	"appgroup": "CORRELATION",	
	"nexus":"ankus-core2-string-similarity-1.1.0.jar",	
	"params":[		
			{
			 "type":"columnselect",
			 "name":"keyIndex",
			 "description":"Identifier Attribute(Index)",
			 "required":"Y",
			 "iotype":"I"		 
			},
			{
			 "type":"columnselect",
			 "name":"computeIndex",
			 "description":"Target Attribute(Index)",
			 "required":"Y",			
			 "iotype":"I"		 
			},
			{
			 "type":"combo",
			 "name":"algorithmOption",
			 "description":"Similarity/Correlation Algorithm",
			 "required":"N",
			 "values":[
				   {"value":"edit","option":"EDIT COEFFICIENT"},				
				   {"value":"hamming","option":"HAMMING DISTANCE"}
				   ], 
			 "iotype":"I"		 
			}		
		]	
	}
  ]
}